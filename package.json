{
  "name": "lazy-list",
  "displayName": "lazy-list",
  "description": "sgsdg",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:lazyList.listItemDone",
    "onCommand:lazyList.listItemOpen",
    "onCommand:lazyList.listItemKill"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lazyList.listItemDone",
        "title": "List Item Done",
        "category": "Lazy List"
      },
      {
        "command": "lazyList.listItemOpen",
        "title": "List Item Open",
        "category": "Lazy List"
      },
      {
        "command": "lazyList.listItemKill",
        "title": "List Item Remove",
        "category": "Lazy List"
      }
    ],
    "keybindings": [
      {
        "command": "lazyList.listItemOpen",
        "key": "ctrl+l ctrl+i",
        "mac": "cmd+l cmd+i"
      },
      {
        "command": "lazyList.listItemDone",
        "key": "ctrl+l ctrl+m",
        "mac": "cmd+l cmd+m"
      },
      {
        "command": "lazyList.listItemKill",
        "key": "ctrl+l ctrl+k",
        "mac": "cmd+l cmd+k"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  }
}
